def find_largest(a, b, c):
    """Finds the largest among three numbers."""
    if a >= b and a >= c:
        return a
    elif b >= a and b >= c:
        return b
    else:
        return c

# Example usage
num1 = 55
num2 = 80
num3 = 30

largest = find_largest(num1, num2, num3)
print(f"The numbers are: {num1}, {num2}, {num3}")
print(f"The largest number is: {largest}")

